pipeline {
    agent any


    stages 
    {  
        
        stage ('git clone') {
        steps{
          git 'https://github.com/reyazmansoori04/maven-project'
         }
    
    }
    
    
    stage('build job'){
    steps{
    withMaven(jdk: 'LocalJDK', maven: 'LocalMaven') {
    sh 'mvn install'
}
    
 }
 }
      stage ('compile job') {
        steps{
        withMaven(maven : 'LocalMaven')
         {
                    sh 'mvn compile'
                }
         }
    
    } 
    
     stage('Get approval'){
                   steps{   input "Deploy to qa?"
                            }}
                            
                            stage('deploy to qa'){
                   steps{   input "deploying"
                            }}
			
    }
    }
