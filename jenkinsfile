pipeline {
    agent any


    stages 
    {  
        
        stage ('git clone') {
        steps{
          git 'https://github.com/reyazmansoori04/maven-project'
         }
    
    }
    
       stage ('git compile') {
        steps{
        withMaven(maven : 'LocalMaven')
         {
                    sh 'mvn compile'
                }
         }
    
    }
    
      stage ('git build') {
        steps{
        withMaven(maven : 'LocalMaven')
         {
                    sh 'mvn build'
                }
         }
    
    }
    
    
      stage ('git test') {
        steps{
        withMaven(maven : 'LocalMaven')
         {
                    sh 'mvn test'
                }
         }
    
    }
}
}
