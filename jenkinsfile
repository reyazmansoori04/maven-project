pipeline {
    agent any

    stages {
               stage ('build') {
		         Steps{
				   echo "hi"
				      }
                }

               stage ('git checkout') {
		         Steps{
				   git '<github url>'
				      }
                 }
               
			   
			     stage ('test') {
				    echo "testing"
                 }
				 
				 stage('Get approval'){
                      input "Deploy to qa?"
                            }
				  stage ('deploy to qa') {
				    echo "deploying"
                 }
				  
				  stage('Get approval'){
                     input "Deploy to prod?"
                        }
						stage('Get approval'){
                     echo "deploying"
                        }
						
            }
        }
