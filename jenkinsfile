pipeline {
    agent any


    stages 
    {  
        
        stage ('git clone') {
        steps{
          git 'https://github.com/reyazmansoori04/maven-project'
         }
    
    }
    
    
    
      stage ('git install') {
        steps{
        withMaven(maven : 'LocalMaven')
         {
                    sh 'mvn install'
                    sh 'echo "Anshul"'
                }
         }
    
    }
    
    
      stage ('git test') {
        steps{
        withMaven(maven : 'LocalMaven')
         {
                    sh 'mvn test'
                }
         }
    
    }
}
}
