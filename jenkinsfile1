pipeline {
    agent any


    stages 
    {  
        
        stage ('git clone') {
        steps{
          git 'https://github.com/reyazmansoori04/maven-project'
         }
    
    }
    
       stage ('build job') {
        steps{
        withMaven(maven : 'LocalMaven')
         {
                    sh 'mvn install'
                }
         }
    
    }
   
}
}
